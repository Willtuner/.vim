#!/usr/bin/env bash

if [[ -z "$1" || -z "$2" ]]; then
    echo >&2 "Usage: $0 file1 file2
      return requirements in file1 but not in file2
  "
    exit 1
fi
comm -23 <(awk -F= '{print $1}' "$1" | sed "1s/$(printf '\357\273\277')//" | tr '[:upper:]' '[:lower:]' | tr '_' '-' | sort) <(awk -F= '{print $1}' "$2" | sed "1s/$(printf '\357\273\277')//" | tr '[:upper:]' '[:lower:]' | tr '_' '-' | sort)
